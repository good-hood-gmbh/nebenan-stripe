  "version": "2.0.0",
  "engines": {
    "node": "^10.x.x"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm run retest && npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "gulp": "gulp",
    "compile": "NODE_ENV=production npm run gulp compile",
    "prepublishOnly": "npm run check && npm run compile",
    "lint": "stylint ./ && eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "retest": "npm run compile && npm test",
    "test": "TZ='GMT' mocha \"test/**/*.js\"",
    "test:tofile": "npm run prepublishOnly && TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log",
    "start": "NODE_ENV=production npm run gulp"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "autoprefixer": "^9.4.3",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-node-env-inline": "0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "browserify-incremental": "^3.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "classnames": "^2.2.6",
    "del": "^3.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "errorhandler": "^1.5.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.16.4",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-stylus": "^2.7.0",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "nebenan-ui-kit": "^2.5.0",
    "nodemon": "^1.18.9",
    "normalize.css": "^8.0.1",
    "postcss-flexbugs-fixes": "^4.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router": "^3.2.1",
    "serve-static": "^1.13.2",
    "sinon": "^7.2.2",
    "stylint": "^1.5.9",
    "stylint-json-reporter": "^0.3.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "classnames": "^2.2.6",
    "nebenan-ui-kit": "^2.5.0"
  },
