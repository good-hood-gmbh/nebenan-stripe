{
  "name": "nebenan-stripe",
  "description": "Use React Stripe Elements without going insane",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
